    <div id="content">
	<table width="100%">
      <tbody>
        <tr>
          <td align="left" valign="top" width="75%">
            <center>
              <h2 align="center">Channel 0 control loop settings</h2>
            </center>
            <hr size="2" width="100%">


              <table width="100%" border="0" cellpadding="2" cellspacing="2">
                <tbody>
                  <tr>
                    <td width="30%" class="gr"></td>
                    <td width="30%" class="gl">Current</td>
                    <td width="30%" class="gl">Updated</td>
					<td width="10%" class="gl"></td>
                  </tr>
                  <tr>
                    <td class="gr">P share: </td>
                    <td><!--#clp0--> mA/K</td>
					<form name="clp" action="clp.cgi" method="get">
					<input name="port" value="0" type="hidden">
                    <td><input maxlength="6" size="7" name="clp">mA/K</td>
					<td>
                      <input name="mysubmit" value="Set" type="submit">
                    </td>
					</form>
                  </tr>
                  <tr>
                    <td class="gr">I share: </td>
                    <td><!--#cli0--> mA/(K*s)</td>
					<form name="cli" action="cli.cgi" method="get">
					<input name="port" value="0" type="hidden">
                    <td><input maxlength="6" size="7" name="cli">mA/(K*s)</td>
					<td>
                      <input name="mysubmit" value="Set" type="submit">
                    </td>
					</form>
                  </tr>
                  <tr>
                    <td class="gr">D share: </td>
                    <td><!--#cld0--> mA*s/K</td>
					<form name="cld" action="cld.cgi" method="get">
					<input name="port" value="0" type="hidden">
                    <td><input maxlength="6" size="7" name="cld">mA*s/K</td>
					<td>
                      <input name="mysubmit" value="Set" type="submit">
                    </td>
					</form>
                  </tr>
                </tbody>
              </table>
			  <br><br>
			  <h3 align="center">Allowed values</h3>
              <table border="1" cellpadding="2" cellspacing="2" width="30%" align="center">
                <tbody>
				  <tr>
                    <td class="grey">P share:</td>
                    <td>-10 000 - 10 000 mA/K</td>
                  </tr>
				  <tr>
                    <td class="grey">I share:</td>
                    <td>-10 000 - 10 000 mA/(K*s)</td>
                  </tr>
				  <tr>
                    <td class="grey">D share:</td>
                    <td>-10 000 - 10 000 mA*s/K</td>
                  </tr>
                </tbody>
              </table>
          </td>
        </tr>
      </tbody>
    </table>
    </div>
    <div id="footing">
      <hr><hr>
    </div>
