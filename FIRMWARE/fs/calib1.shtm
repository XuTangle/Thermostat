		<form name="saveall" action="saveall.cgi" method="get">
					<input name="saveall" value="1" type="hidden">
					<td>
                      <button name="mysubmit" value="save">Save current parameters</button>
                    </td>
		</form>
		<form name="soft_reset" action="soft_reset.cgi" method="get">
					<input name="soft_reset" value="1" type="hidden">
					<td>
                      <button name="mysubmit" value="Reset">Reset device</button>
                    </td>
		</form>
      </ul>
    </div>
    <div id="content">
	<table width="100%">
      <tbody>
        <tr>
          <td align="left" valign="top" width="75%">
            <center>
              <h2 align="center">Channel 1 Calibration</h2>
            </center>
            <hr size="2" width="100%">

              <table width="100%" border="0" cellpadding="2" cellspacing="2">
                <tbody>
                  <tr>
                    <td width="30%" class="gr"></td>
                    <td width="30%" class="gl">Current</td>
                    <td width="30%" class="gl">Updated</td>
					<td width="10%" class="gl"></td>
                  </tr>
                  <tr>
                    <td class="gr">Type of temperature sensor: </td>
                    <td><!--#tempmod1--> </td>
					<form name="calib" action="calibmode.cgi" method="get">
					<input name="port" value="1" type="hidden">
                    <td>
                      <select size="1" name="tempmode">
                        <option value="0">NTC/PTC</option>
                        <option value="1">Pt100/Pt1000</option>
                      </select>
                    </td>
					<td>
                      <input name="mysubmit" value="Set" type="submit">
                    </td>
					</form>
                  </tr>
                  <tr>
                    <td class="gr">T0: </td>
                    <td><!--#t01--> °C</td>
					<form name="calib" action="calibt0.cgi" method="get">
					<input name="port" value="1" type="hidden">
                    <td><input maxlength="6" size="7" name="t0">°C</td>
					<td>
                      <input name="mysubmit" value="Set" type="submit">
                    </td>
					</form>
                  </tr>
                  <tr>
                    <td class="gr">Beta: </td>
                    <td><!--#beta1--> K</td>
					<form name="calib" action="calibbeta.cgi" method="get">
					<input name="port" value="1" type="hidden">
                    <td><input maxlength="8" size="9" name="beta">K</td>
					<td>
                      <input name="mysubmit" value="Set" type="submit">
                    </td>
					</form>
                  </tr>
                  <tr>
                    <td class="gr">Ratio: </td>
                    <td><!--#ratio1--> </td>
					<form name="calib" action="calibratio.cgi" method="get">
					<input name="port" value="1" type="hidden">
                    <td><input maxlength="8" size="9" name="ratio"></td>
					<td>
                      <input name="mysubmit" value="Set" type="submit">
                    </td>
					</form>
                  </tr>
                 <tr>
                    <td class="gr">A: </td>
                    <td><!--#pta1--> × 10<sup>-6</sup> / °C </td>
					<form name="calib" action="calibpta.cgi" method="get">
					<input name="port" value="1" type="hidden">
                    <td><input maxlength="8" size="9" name="pta">× 10<sup>-6</sup> / °C</td>
					<td>
                      <input name="mysubmit" value="Set" type="submit">
                    </td>
					</form>
                  </tr>
                  <tr>
                    <td class="gr">B: </td>
                    <td><!--#ptb1--> × 10<sup>-9</sup> / °C<sup>2</sup> </td>
					<form name="calib" action="calibptb.cgi" method="get">
					<input name="port" value="1" type="hidden">
                    <td><input maxlength="8" size="9" name="ptb">× 10<sup>-9</sup> / °C<sup>2</sup></td>
					<td>
                      <input name="mysubmit" value="Set" type="submit">
                    </td>
					</form>
                  </tr>
                  <tr>
                    <td class="gr">Switching frequency of TEC driver: </td>
                    <td><!--#swfreq1--> kHz</td>
					<form name="calib" action="calibswfreq.cgi" method="get">
					<input name="port" value="1" type="hidden">
                    <td>
                      <select size="1" name="swfreq">
                        <option value="500">500 kHz</option>
                        <option value="1000">1 MHz</option>
                      </select>
                    </td>
					<td>
                      <input name="mysubmit" value="Set" type="submit">
                    </td>
					</form>
                  </tr>
                  <tr>
                    <td class="gr">Raw temperature value: </td>
                    <td><!--#rawtemp1--> </td>
                    <td></td>
                  </tr>
                </tbody>
              </table>
			  <br><br>
			  <h3 align="center">Allowed values</h3>
              <table border="1" cellpadding="2" cellspacing="2" width="35%" align="center">
                <tbody>
                  <tr>
                    <td class="grey">T0:</td>
                    <td>0.000 - 50.000 °C</td>
                  </tr>
                  <tr>
                    <td class="grey">Beta:</td>
                    <td>-20000 - 20000</td>
                  </tr>
                  <tr>
                    <td class="grey">Ratio:</td>
                    <td>0.000 - 100.000</td>
                  </tr>
                  <tr>
                    <td class="grey">A:</td>
                    <td>-20000 - 20000 × 10<sup>-6</sup> / °C</td>
                  </tr>
                  <tr>
                    <td class="grey">B:</td>
                    <td>-20000 - 20000 × 10<sup>-9</sup> / °C<sup>2</sup></td>
                  </tr>
                  <tr>
                    <td class="grey">Switching frequency:</td>
                    <td>500 kHz / 1 MHz</td>
                  </tr>
                </tbody>
              </table>
			  <h3 align="center">Description</h3>
              <table border="1" cellpadding="2" cellspacing="2" width="55%" align="center">
                <tbody>
                  <tr>
                    <td>T0 - reference temperature of thermistor</td>
                  </tr>
                  <tr>
                    <td>Beta - parameter indicating shape of R-T curve, not used in Pt100/Pt1000 sensor</td>
                  </tr>
                  <tr>
                    <td>Ratio - resistance of thermistor at T0 / reference resistance (10k)</td>
                  </tr>
				  <tr>
                    <td>A -  linear coefficient of Pt100/Pt1000 equation, not used in NTC/PTC sensor</td>
                  </tr>
				  <tr>
                    <td>B -  quadratic coefficient of Pt100/Pt1000 equation, not used in NTC/PTC sensor</td>
                  </tr>
                </tbody>
              </table>
          </td>
        </tr>
      </tbody>
    </table>
    </div>
    <div id="footing">
      <hr><hr>
    </div>
  </body>
</html>
