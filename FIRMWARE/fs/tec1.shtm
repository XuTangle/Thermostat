    <div id="content">
	<table width="100%">
      <tbody>
        <tr>
          <td align="left" valign="top" width="75%">
            <center>
              <h2 align="center">Channel 1 TEC Settings</h2>
            </center>
            <hr size="2" width="100%">

              <table width="100%" border="0" cellpadding="2" cellspacing="2">
                <tbody>
                  <tr>
                    <td width="30%" class="gr"></td>
                    <td width="30%" class="gl">Current</td>
                    <td width="30%" class="gl">Updated</td>
					<td width="10%" class="gl"></td>
                  </tr>
                  <tr>
                    <td class="gr">TEC positive current limit: </td>
                    <td><!--#teclimp1--> mA</td>
					<form name="teclim" action="teclim.cgi" method="get">
					<input name="port" value="1" type="hidden">
					<input name="posneg" value="0" type="hidden">
                    <td><input maxlength="4" size="5" name="teclim">mA</td>
					<td>
                      <input name="mysubmit" value="Set" type="submit">
                    </td>
					</form>
                  </tr>
                  <tr>
                    <td class="gr">TEC negative current limit: </td>
                    <td><!--#teclimn1--> mA</td>
					<form name="teclim" action="teclim.cgi" method="get">
					<input name="port" value="1" type="hidden">
					<input name="posneg" value="1" type="hidden">
                    <td><input maxlength="4" size="5" name="teclim">mA</td>
					<td>
                      <input name="mysubmit" value="Set" type="submit">
                    </td>
					</form>
                  </tr>
                  <tr>
                    <td class="gr">TEC voltage limit: </td>
                    <td><!--#maxvolt1--> mV</td>
					<form name="maxvolt" action="maxvolt.cgi" method="get">
					<input name="port" value="1" type="hidden">
                    <td><input maxlength="4" size="5" name="maxvolt">mV</td>
					<td>
                      <input name="mysubmit" value="Set" type="submit">
                    </td>
					</form>
                  </tr>
                  <tr>
                    <td class="gr">Actual TEC current: </td>
                    <td><!--#teccur1--> mA</td>
                    <td></td>
                  </tr>
                  <tr>
                    <td class="gr">TEC reference voltage: </td>
                    <td><!--#tecvol1--> mV</td>
                    <td></td>
                  </tr>
                </tbody>
              </table>
			  <br><br>
			  <table border="0" cellpadding="2" cellspacing="0" width="50%" align="center">
                <tbody>
                  <tr>
                    <td>TEC driver is currently <!--#onoff1--> </td>
					<form name="turnonoff" action="turnonoff.cgi" method="get">
					<input name="port" value="1" type="hidden">
                    <td><button name="onoff" value="1">Turn it <!--#onoff1R--></button></td>
					</form>
                  </tr>
                </tbody>
              </table>
			 <br><br>
			  <h3 align="center">Allowed values</h3>
              <table border="1" cellpadding="2" cellspacing="2" width="35%" align="center">
                <tbody>
                  <tr>
                    <td class="grey">TEC positive current limit:</td>
                    <td>0 - 3 000 mA</td>
                  </tr>
                  <tr>
                    <td class="grey">TEC negative current limit:</td>
                    <td>0 - 3 000 mA</td>
                  </tr>
                  <tr>
                    <td class="grey">TEC voltage limit:</td>
                    <td>0 - 5 000 mV</td>
				  </tr>
                </tbody>
              </table>
          </td>
        </tr>
      </tbody>
    </table>
    </div>
    <div id="footing">
      <hr><hr>
    </div>
